<div class="page-header">
  <%= link_to :back, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-arrow-left"></span>
    Back
  <% end %>
  <%= link_to edit_supplement_path(@supplement), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show supplement</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @supplement.title %></dd>

  <dt>Image url:</dt>
  <dd><%= @supplement.image_url %></dd>

  <dt>Url:</dt>
  <dd><%= @supplement.url %></dd>

  <dt>Diet:</dt>
  <dd><%= @supplement.diet_id %></dd>

</dl>
